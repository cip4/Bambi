<?xml version="1.0" encoding="UTF-8"?>
  <!--
    SlaveURL - the URL of a slave to push to 
    SlaveStatus - the method to query for slave status - JMF, JMFGLOBAL, NodeInfo
    (missing=none)
     SlaveDeviceID=the device id of the slave
  -->
 
<application 
Port="8080" 
JettyPort="44483"

Context="/BambiProxy"
BaseDir="/bambidata/bambiproxy/" 
SenderID="ProxyDevice" 
JDFDir="JDFDir" 
PropertiesName="org.cip4.bambi.proxy.ProxyProperties"
DeviceClass="org.cip4.bambi.proxy.ProxyDevice">
  
  <Device DeviceID="proxyRoot" 
  DeviceType="Bambi Proxy Root" 
  <Device 
  DeviceID="pullproxy" 
  DeviceType="Bambi Pull Proxy for SimWorker/sim001" 
  SlaveStatus="JMFGLOBAL"
    SlaveDeviceID="sim001" 
    SlaveURL="http://localhost:8080/SimWorker/jmf/sim001"
    MaxPush="0" 
    InputHF="/var/bambi/hf/pullproxy/Input" 
    OutputHF="/var/bambi/hf/pullproxy/Output" 
    ErrorHF="/var/bambi/hf/pullhproxy/Error"/>
  
  <Device DeviceID="pushproxy" 
  DeviceType="Bambi Push Proxy for SimWorker/sim003" 
  SlaveStatus="JMFGLOBAL"
    SlaveDeviceID="sim003" 
    SlaveURL="http://localhost:8080/SimWorker/jmf/sim003"
    MaxPush="10" 
    InputHF="/var/bambi/hf/pushproxy/Input" 
    OutputHF="/var/bambi/hf/pushproxy/Output" 
    ErrorHF="/var/bambi/hf/pushproxy/Error"/>

